{
  "manifest_version": 3,
  "name": "Tab Saver",
  "description": "Save tabs and windows in tab folders",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}